<section class="campaigns">
    <h2 class="campaigns__head" data-cam-anime-head>
        <div class="campaigns__head__main" data-apng-id="textdododo" data-apng-src="<%= imgDir %>anime_head.png"></div>
        <img src="<%= imgDir %>txt_head.webp" alt="なキャンペーンを見逃すな！" width="390" height="227" loading="lazy">
    </h2>
    <div class="campaigns__items">
        <% items.forEach((item) => { %>
        <article class="campaigns__item" aria-expanded="false" data-accordion-parent="<%= item.id %>" data-cam-anime-article="">
            <div class="campaigns__badge" data-cam-anime-badge="">
                <img src="<%= imgDir %>badge_<%= item.id %>.webp" alt="" width="72" height="72" loading="lazy">
            </div>
            <div class="campaigns__inner" id="campaign<%= item.id %>">
                <div class="campaigns__contents" data-accordion="<%= item.id %>">
                    <h3 class="campaigns__title">
                        <span class="campaigns__title__sub">
                            <img src="<%= imgDir %>txt_title_sub_<%= item.id %>.webp" alt="<%= item.title.sub %>" width="390" height="128" loading="lazy">
                        </span>
                        <span class="campaigns__title__main" data-cam-anime-title="">
                            <img src="<%= imgDir %>txt_title_main_<%= item.id %>.webp" alt="<%= item.title.main %>" width="268" height="89" loading="lazy">
                        </span>
                    </h3>
                    <div class="campaigns__img">
                        <img src="<%= imgDir %>ph_main_<%= item.id %>.webp" alt="" data-cam-anime-image="">
                        <div class="campaigns__img__frame" data-cam-anime-image=""></div>
                    </div>
                    <div class="campaigns__texts">
                        <div class="campaigns__date"><%= item.date %></div>
                        <div class="campaigns__head"><%= item.head %></div>
                        <div class="campaigns__body"><%= item.body %></div>
                        <a href="<%= item.link.href %>" target="_blank" rel="noopener noreferrer" class="campaigns__link">
                            <img src="<%= imgDir %>ph_btn_link_<%= item.id %>.webp" alt="" width="320" height="70" alt="<%= item.link.text %>" loading="lazy">
                        </a>
                    </div>
                </div>
            </div>
            <button type="button" class="campaigns__trigger" data-accordion-trigger="<%= item.id %>" aria-controls="campaign<%= item.id %>"><span class="campaigns__trigger__open">くわしく見る</span><span class="campaigns__trigger__close">閉じる</span></button>
        </article>
        <% }) %>
    </div>
</section>
